<header class="Header wrapper">

  <nav class="Header-secondaryNav" role="navigation">
    <ul class="List List--inline">
      {% for item in secondary_menu %}
        <li><a href="{{item.url}}">{{item.title}}</a></li>
      {% endfor %}
    </ul>
    <a class="Header-search" href="">
      <svg aria-hidden="true" role="presentation" title="Search Our Website">
        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#search"></use>
      </svg>
    </a>
  </nav>

  <nav class="Header-primaryNav" role="navigation">
    <div class="Header-logo">
      <svg aria-hidden="true" role="presentation" title="">
        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#logo"></use>
      </svg>
    </div>
    <ul class="List List--inline">
      {% for item in primary_menu %}
        <li><a href="{{item.url}}">{{item.title}}</a></li>
      {% endfor %}
    </ul>
    <button type="button" class="Button Header-quickNav" href="">More Info</button>
  </nav>

</header>
